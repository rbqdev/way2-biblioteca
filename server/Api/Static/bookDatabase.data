    "Id": 147,
    "Title": "Governança Corporativa no Brasil e no Mundo",
    "AuthorName": "Alexandre Di Miceli da Silveira",
    "ISBN": "8535236481",
    "ISBN13": "978-8535236484",
    "GoodreadsId": null,
    "ImageUrl": "/images/books/147.jpg",
    "SmallImageUrl": "/images/books/147.jpg",
    "PublicationDate": "2015-01-01T00:00:00",
    "Publisher": null,
